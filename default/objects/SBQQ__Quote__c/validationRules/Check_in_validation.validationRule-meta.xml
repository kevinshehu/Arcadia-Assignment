<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_in_validation</fullName>
    <active>true</active>
    <errorConditionFormula>Check_in__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Check_in__c</errorDisplayField>
    <errorMessage>The Check-in date cannot be in the past.</errorMessage>
</ValidationRule>
